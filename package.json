{
    "name": "dbc-snippets",
    "publisher": "David-Garcia",
    "displayName": "dbc-snippets",
    "description": "Snippets for DBC",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dgghun/dbc-snippets.git"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "dbc",
                "aliases": ["DBC", "dbc"],
                "extensions": [".DGG",".TXT",".DEF",".VRB",".DAV",".GLE",".GWS",".AJS",".MAG"],
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
