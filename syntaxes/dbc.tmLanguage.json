{
	"$schema": "../tmlanguage.json",
	"name": "DBC",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comments"	
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
